    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "joi": "^17.6.0",
    "morgan": "^1.10.0",
    "redis": "^4.3.1"
  },
